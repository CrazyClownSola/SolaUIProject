<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="EdgeLoadingView">
        <attr name="edge_cycle_duration" format="integer"/>
        <attr name="edge_cycle_radius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="DragRoundView">
        <attr name="dg_text_size" format="dimension"/>
        <attr name="dg_text_color" format="color"/>
        <attr name="dg_round_radius" format="dimension"/>
        <attr name="dg_round_background" format="color"/>
        <attr name="dg_size_offset" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="WoWPathView">
        <attr name="pathColor" format="color"/>
        <attr name="pathWidth" format="float"/>
        <attr name="headImageId" format="integer"/>
        <attr name="headImageWidth" format="float"/>
        <attr name="headImageHeight" format="float"/>
    </declare-styleable>

</resources>